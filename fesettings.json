{
    "version": "1.0.0",
    "parentDirectoryPath": "C:\\",
    "global": {
        "readSubFolders": true,
        "ignoreFolders": ["node_modules", "STG"]
    },
    "search": {
        "ignoreFileTypes": ["asar", "dat", "db", "dll", "exe", "heic", "jpeg", "jpg", "map", "md", "mjs", "mp3", "mp4", "pack", "png", "xml"],
        "searchFileTypes": ["html"],
        "useIFT": true,
        "useSFT": false
    },
    "html": {
        "headOrder": ["title", "meta", "link", "script"]
    }
}